{
    "name": "unicourse-frontend",
    "version": "0.1.0",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview --port 3000",
        "format": "prettier --write \"**/*.{js,ts,css,html,vue,json,yml,yaml}\" \"!dist\" \"!storybook-static\"",
        "lint": "eslint .",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "prepare": "chmod +x node_modules/husky/lib/bin.js && husky install",
        "build:docker": "docker build -t unicourse-frontend ."
    },
    "dependencies": {
        "marked": "^4.2.2",
        "pinia": "^2.0.23",
        "sweetalert2": "^11.6.10",
        "unicourse": "1.0.0-alpha.13",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@iconify-json/ion": "^1.1.4",
        "@iconify-json/octicon": "^1.1.23",
        "@storybook/addon-actions": "^6.5.13",
        "@storybook/addon-essentials": "^6.5.13",
        "@storybook/addon-links": "^6.5.13",
        "@storybook/builder-vite": "^0.2.5",
        "@storybook/core-common": "^6.5.13",
        "@storybook/vue3": "^6.5.13",
        "@types/marked": "^4.0.7",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vue/compiler-sfc": "^3.2.45",
        "@vueuse/head": "^1.0.0",
        "autoprefixer": "^10.4.13",
        "babel-loader": "^8.3.0",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.2",
        "postcss": "^8.4.19",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "pretty-quick": "^3.1.3",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.8.4",
        "unplugin-auto-import": "^0.11.4",
        "unplugin-icons": "^0.14.13",
        "unplugin-vue-components": "^0.22.9",
        "vite": "^3.2.3",
        "vite-plugin-pages": "^0.27.1",
        "vue-loader": "^17.0.1",
        "vue-tsc": "^1.0.9"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "react",
                "react-dom",
                "webpack"
            ]
        }
    },
    "vite": {
        "mode": "development"
    }
}
